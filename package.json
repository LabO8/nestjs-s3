{
  "name": "@nestjsplus/nestjs-package-starter",
  "version": "1.0.0",
  "description": "NestJS npm package starter",
  "author": "John Biundo <johnfbiundo@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/test.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "npx husky install"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjsplus/nestjs-package-starter"
  },
  "bugs": "https://github.com/nestjsplus/nestjs-package-starter",
  "peerDependencies": {
    "@nestjs/common": "^8.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.34.0",
    "@aws-sdk/node-http-handler": "^3.34.0",
    "@aws-sdk/s3-request-presigner": "^3.34.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@hkdobrev/run-if-changed": "^0.3.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/testing": "^8.0.6",
    "@types/express": "4.16.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.16.1",
    "@types/supertest": "2.0.7",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "commitlint": "^13.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7",
    "supertest": "4.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "~9.1.1",
    "tsc-watch": "4.5.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "~4.2.4"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint --fix",
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "run-if-changed": {
    "yarn.lock": "yarn install"
  }
}
